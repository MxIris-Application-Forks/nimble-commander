    "terminalAnsiColorB": "#E5E500", // bright yellow
    "terminalAnsiColorC": "#0000FF", // bright blue
    "terminalAnsiColorD": "#E500E5", // bright magenta
    "terminalAnsiColorE": "#00E5E5", // bright cyan
    "terminalAnsiColorF": "#E5E5E5"  // bright white
    
