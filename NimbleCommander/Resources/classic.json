{
    "themeAppearance": "aqua",

     "filePanelsColoringRules_v1": [
                                 {
                                    "name": "selected",
                                    "regular": "#FFFF00",
                                    "focused": "#FFFF00",
                                    "filter": {
                                        "selected": true
                                    }
                                 },
                                 {
                                    "name": "hidden",
                                    "regular": "#008080",
                                    "focused": "#808080",
                                    "filter": {
                                        "hidden": true
                                    }
                                 },
                                 {
                                    "name": "regular files",
                                    "regular": "#00FFFF",
                                    "focused": "#000000",
                                    "filter": {
                                        "reg": true
                                    }
                                 },
                                 {
                                    "name": "directories",
                                    "regular": "#FFFFFF",
                                    "focused": "#FFFFFF",
                                    "filter": {
                                        "directory": true
                                    }
                                 },
                                 {
                                    "name": "other",
                                    "regular": "#FF0000",
                                    "focused": "#FF0000"
                                 }
    ],
    "filePanelsGeneralDropBorderColor": "@yellowColor",
    "filePanelsGeneralOverlayColor": "@whiteColor",    
    
    /* File Panels -> tabs settings */
    "filePanelsTabsFont": "Menlo-Regular, 11",
    "filePanelsTabsTextColor": "#E5E5E5",
