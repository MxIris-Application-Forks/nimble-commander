/* Title for copying or moving a multiple items */
"%@ %@ items to “%@”" = "%1$@ %2$@ объектов в “%3$@”";

/* Title for copying or moving a single item */
"%@ “%@” to “%@”" = "%@ “%@” в “%@”";

/* User action button title to abort an operation */
"Abort" = "Прервать";

/* Operation title prefix for copying */
"Copying" = "Копирование";

/* Title for error wheen when destination file already exists */
"Destination already exists. Do you want to rewrite it?" = "Итоговый файл уже существует. Вы хотите переписать его?";

/* Informative text on case when destination file already exists */
"Destination: %@\nSource: %@" = "Итоговый путь: %1$@\nИсходный путь: %2$@";

/* Error informative text with path */
"Error: %@\nPath: %@" = "Ошибка: %1$@\nПуть: %2$@";

/* Title on error when source file is inaccessible */
"Failed to access file" = "Отсутствует доступ к файлу";

/* Error sheet prompt on dir creation */
"Failed to create directory" = "Не удалось создать папку";

/* Title on error when destination file can't be opened */
"Failed to open destination file" = "Не удалось открыть целевой файл";

/* Error when reading failed */
"Failed to read a file" = "Не удалось прочитать файл";

/* Error when writing failed */
"Failed to write a file" = "Не удалось записать файл";

/* User action button title to hide an error sheet */
"Hide" = "Скрыть";

/* Operaration title prefix for moving */
"Moving" = "Перенос";

/* Title for processing a single item */
"Processing “%@”" = "Обработка “%@”";

/* User action button title to rewrite a file */
"Rewrite" = "Переписать";

